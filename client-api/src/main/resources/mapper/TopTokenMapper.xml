<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ruoyi.web.mapper.TopTokenMapper">

    <select id="selectOnlineListVO" resultType="com.ruoyi.web.vo.TokenVO">
        select tt.*
        from top_token tt
        where tt.online = 1
    </select>

</mapper>