<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.web.mapper.TopTokenBurningMapper">

    <select id="selectBySymbol" resultType="com.ruoyi.web.vo.TokenBurningVO">
        select *
        from top_token_burning
        where symbol = #{symbol}
    </select>

</mapper>

